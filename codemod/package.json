  "devDependencies": {
    "@types/inquirer": "^7.3.3",
    "@types/is-git-clean": "^1.1.0",
    "@types/jscodeshift": "^0.11.7",
    "@types/node": "^20.8.4",
    "rimraf": "^5.0.5",
    "typescript": "^5.2.2"
  }
